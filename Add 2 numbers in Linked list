#include<bits/stdc++.h>
using namespace std;
class Node
{
    public:
    int data;
    Node* next;
    public:
    Node(int data1,Node* next1)
    {
        data=data1;
        next=next1;
      
    }
    public:
    Node(int data1)
    {
        data=data1;
        next=nullptr;
    }
};
Node* conv(vector<int>k)
{
    Node* head=new Node(k[0]);
    Node* mov=head;
    for(int i=1;i<k.size();i++)
    {
       Node* temp=new Node(k[i]);
       mov->next=temp;
       mov=temp;

    }
    return head;

}
Node* addtwoll(Node* head1,Node* head2)
{
    int carry=0;
    Node* dummy=new Node(-1);
    Node* current=dummy;
    int s=0;
    while(head1!=NULL || head2!=NULL)
    {
          s=carry;
          if(head1)
          {
            s=s+head1->data;
          }
          if(head2)
          {
            s=s+head2->data;
          }
          Node* newnode=new Node(s%10);
          current->next=newnode;
          current=newnode;
          carry=s/10;
          if(head1)
          {
            head1=head1->next;
          }
          if(head2)
          {
            head2=head2->next;
          }
    }
    return dummy->next;
}

int main()
{
    vector<int>k={1,2,3};
    vector<int>l={2,3,4};
    Node* head=conv(k);
    Node* head1=conv(l);
    Node* newnode=addtwoll(head,head1);
    while(newnode)
    {
       cout<<newnode->data;
       newnode=newnode->next;
    }
       
    
}
