
#include <bits/stdc++.h>
using namespace std;
int  linear_search(int a[],int n,int h)
{
     for(int i=0;i<n;i++)
     {
     	if(a[i]==h)
     	{
     		return i;
		 }
	 }
	 return -1;


}
int main() {
   int n;
   cin>>n;
   int a[n];
   for(int i=0;i<n;i++)
   {
       cin>>a[i];
   }
 int h;
 cin>>h;
  int k=linear_search(a,n,h);
  if(k==-1)
  {
  	cout<<"NOT FOUND AT THE ARRAY";
  }
  else
  {
  	cout<<"ELEMENT FOUND AT THE INDEX OF: "<<k;
  }
  
}
